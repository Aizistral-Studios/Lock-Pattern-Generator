{
	"extends": "@vue/tsconfig/tsconfig.web.json",
	"include": [
		"env.d.ts",
		"src/**/*",
		"src/**/*.vue"
	],
	"compilerOptions": {
		"skipLibCheck": true,
		"baseUrl": ".",
		"paths": {
			"@/*": [
				"./src/*"
			]
		}
	},
	"references": [
		{
			"path": "./tsconfig.config.json"
		}
	],
	"exclude": [
		"node_modules"
	]
}